<UserControl x:Class="Matrix.UserDefinedMatrix"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="434" d:DesignWidth="758">
    
    <Grid x:Name="LayoutRoot" Height="430" Width="754" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Image Height="418" HorizontalAlignment="Left" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="7" Source="/Matrix;component/Images/Untitled1.png" />
        <Image Height="418" HorizontalAlignment="Left" Margin="735,0,0,0" Name="image2" Source="/Matrix;component/Images/Untitled1.png" Stretch="Fill" VerticalAlignment="Top" Width="7" FlowDirection="LeftToRight" />
        <Grid Height="415" HorizontalAlignment="Left" Margin="12,0,0,0" Name="grid1" VerticalAlignment="Top" Width="717" ShowGridLines="False" Background="{x:Null}"></Grid>
    </Grid>
</UserControl>
