<UserControl x:Class="Matrix.UserControls.SingleResult"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480" HorizontalAlignment="Center" VerticalAlignment="Center">
    
    <Grid x:Name="LayoutRoot" Height="72" Width="182">
        <TextBlock Height="30" HorizontalAlignment="Left" Margin="11,20,0,0" Name="textBlock1" Text="Ans:" VerticalAlignment="Top" />
        <TextBox Background="{x:Null}" BorderBrush="{x:Null}" Height="72" HorizontalAlignment="Left" Margin="55,0,0,0" Name="textBox1" Text="TextBox" VerticalAlignment="Top" Width="127" />
    </Grid>
</UserControl>
